@model DAL.Modelo.TdcCatLineasDistribucion

@{
    ViewData["Title"] = "Delete";
}

<!-- Página para eliminar los campos de las líneas de distribución -->

<h3>¿Seguro que quieres eliminarlo?</h3>

<div>
    <h4>Líneas de distribución</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.MdUuid)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MdUuid)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.MdDate)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.MdDate)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Id)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Id)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CodProvincia)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CodProvincia)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CodMunicipio)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CodMunicipio)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CodBarrio)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CodBarrio)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="CodLinea" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-action="Index">Volver</a>
    </form>
</div>
