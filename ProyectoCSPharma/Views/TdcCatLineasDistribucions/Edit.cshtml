@model DAL.Modelo.TdcCatLineasDistribucion

@{
    ViewData["Title"] = "Edit";
}

<!-- Página que edita las categorías de las líneas de distribución -->

<h1>Editar</h1>

<h4>Líneas de distribución</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="MdUuid" class="control-label"></label>
                <input asp-for="MdUuid" class="form-control" />
                <span asp-validation-for="MdUuid" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="MdDate" class="control-label"></label>
                <input asp-for="MdDate" class="form-control" />
                <span asp-validation-for="MdDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Id" class="control-label"></label>
                <input asp-for="Id" class="form-control" />
                <span asp-validation-for="Id" class="text-danger"></span>
            </div>
            <input type="hidden" asp-for="CodLinea" />
            <div class="form-group">
                <label asp-for="CodProvincia" class="control-label"></label>
                <input asp-for="CodProvincia" class="form-control" />
                <span asp-validation-for="CodProvincia" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CodMunicipio" class="control-label"></label>
                <input asp-for="CodMunicipio" class="form-control" />
                <span asp-validation-for="CodMunicipio" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CodBarrio" class="control-label"></label>
                <input asp-for="CodBarrio" class="form-control" />
                <span asp-validation-for="CodBarrio" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Volver</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
