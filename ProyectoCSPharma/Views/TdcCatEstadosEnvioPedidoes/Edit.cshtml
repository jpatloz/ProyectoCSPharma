@model DAL.Modelo.TdcCatEstadosEnvioPedido

@{
    ViewData["Title"] = "Edit";
}

<!-- Página que edita los campos del envio de devolución del pedido -->

<h1>Editar</h1>

<h4>Estado del envío del pedido</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="CodEstadoEnvio" />
            <div class="form-group">
                <label asp-for="MdDate" class="control-label"></label>
                <input asp-for="MdDate" class="form-control" />
                <span asp-validation-for="MdDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="MdUuid" class="control-label"></label>
                <input asp-for="MdUuid" class="form-control" />
                <span asp-validation-for="MdUuid" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DesEstadoEnvio" class="control-label"></label>
                <input asp-for="DesEstadoEnvio" class="form-control" />
                <span asp-validation-for="DesEstadoEnvio" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Volver</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
